{
  "name": "startup-car-fullstack",
  "version": "1.0.0",
  "description": "Sistema completo de aluguel de carros com frontend Next.js e backend Spring Boot",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd front && npm run dev",
    "dev:backend": "cd back && mvn spring-boot:run",
    "build": "cd front && npm run build",
    "install:frontend": "cd front && npm install",
    "install:all": "npm run install:frontend",
    "start": "npm run dev"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "nextjs",
    "spring-boot",
    "postgresql",
    "crud",
    "car-rental"
  ],
  "author": "Xumblaubs Team",
  "license": "MIT"
}
